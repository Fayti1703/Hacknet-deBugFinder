<?xml version="1.0" encoding="utf-8" ?>
<PatcherCommands xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="TaskList.xsd">
	<namespace name="Hacknet">
		<type name="MainMenu">
			<field name="titleColor" access="internal" />
			<field name="titleFont" access="internal" />
		</type>
		<type name="ThemeManager">
			<field name="backgroundImage" access="internal" />
			<field name="backgroundNeedsDisposal" access="internal" />
			<field name="customBackgroundImageLoadPath" access="internal" />
			<method name="switchThemeLayout" access="internal" />
		</type>
		<namespace name="Effects">
			<type name="ActiveEffectsUpdater">
				<field name="oldThemePath" access="internal" />
				<field name="newThemePath" access="internal" />
			</type>
		</namespace>
	</namespace>
</PatcherCommands>
